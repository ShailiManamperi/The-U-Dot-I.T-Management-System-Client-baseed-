         <image>
            <Image url="@../../Asstes/System/employee.jpg" />
         </image>
      </ImageView>
      <JFXTextField fx:id="txtSearch" layoutX="117.0" layoutY="68.0" onAction="#searchOnAction" prefHeight="37.0" prefWidth="272.0" promptText="Search">
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
      <ImageView fx:id="imgsearch" fitHeight="31.0" fitWidth="28.0" layoutX="83.0" layoutY="73.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Asstes/System/icons8-search-100.png" />
         </image>
      </ImageView>
      <HBox layoutX="22.0" layoutY="604.0" prefHeight="30.0" prefWidth="500.0" spacing="20.0">
         <children>
            <JFXButton fx:id="btnAdd" onAction="#addstudentOnAction" prefHeight="65.0" prefWidth="107.0" text="Add">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btnUpdate" onAction="#UpdatestudentOnAction" prefHeight="65.0" prefWidth="108.0" text="Update">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btnDelete" onAction="#DeletestudentOnAction" prefHeight="65.0" prefWidth="109.0" text="Delete">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="btnClear" onAction="#ClearstudentOnAction" prefHeight="65.0" prefWidth="109.0" text="Clear">
               <font>
                  <Font name="System Bold" size="18.0" />

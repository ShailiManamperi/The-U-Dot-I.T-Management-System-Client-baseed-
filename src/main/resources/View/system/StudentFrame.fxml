            <Image url="@../../Asstes/System/logo1.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgStud" fitHeight="250.0" fitWidth="250.0" layoutX="580.0" layoutY="139.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Asstes/System/logo1.png" />
         </image>
      </ImageView>
      <JFXButton fx:id="btnupload" layoutX="839.0" layoutY="326.0" onAction="#uploadimageOnAction" prefHeight="25.0" prefWidth="103.0" text="Upload ">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </JFXButton>
      <ImageView fx:id="imgQr" fitHeight="250.0" fitWidth="250.0" layoutX="579.0" layoutY="410.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Asstes/System/logo1.png" />
         </image>
      </ImageView>
      <JFXButton fx:id="btngenerate" layoutX="843.0" layoutY="598.0" onAction="#generateQrOnAction" prefHeight="25.0" prefWidth="103.0" text="Generate">

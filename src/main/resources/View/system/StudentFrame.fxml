<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="frame" prefHeight="700.0" prefWidth="985.0" style="-fx-background-color: #e5e5e6;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.system.it.Controller.StudentFrameController">
   <children>
      <ImageView fitHeight="440.0" fitWidth="388.0" layoutX="73.0" layoutY="114.0" opacity="0.37" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Asstes/System/employee.jpg" />
         </image>
      </ImageView>
      <VBox layoutX="56.0" layoutY="97.0" prefHeight="468.0" prefWidth="423.0" spacing="35.0">
         <children>
            <JFXTextField fx:id="txtStudid" prefHeight="37.0" prefWidth="333.0" promptText="Student id">
               <font>
                  <Font size="16.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="txtstudename" prefHeight="37.0" prefWidth="333.0" promptText="Student Name">
               <font>
                  <Font size="16.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="txtcontactno" prefHeight="37.0" prefWidth="333.0" promptText="Contact number">
               <font>
                  <Font size="16.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="txtaddress" prefHeight="37.0" prefWidth="333.0" promptText="Address">
               <font>
                  <Font size="16.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="txtschool" prefHeight="37.0" prefWidth="333.0" promptText="School">
               <font>
                  <Font size="16.0" />
               </font>
            </JFXTextField>
            <JFXComboBox fx:id="cmbcourse" prefHeight="50.0" prefWidth="396.0" promptText="Course" />
            <JFXComboBox fx:id="cmbclass" prefHeight="50.0" prefWidth="396.0" promptText="Class" />
         </children>
         <padding>
            <Insets bottom="10.0" left="15.0" right="15.0" top="20.0" />
         </padding>
      </VBox>
      <Label fx:id="lbltitle" layoutX="94.0" layoutY="14.0" prefHeight="43.0" prefWidth="308.0" text="Student Management" textFill="#30394a">
         <font>
            <Font name="High Tower Text Italic" size="35.0" />
         </font>
      </Label>
      <ImageView fitHeight="75.0" fitWidth="75.0" onMouseClicked="#HomePageOnMouseClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Asstes/System/logo1.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgStud" fitHeight="250.0" fitWidth="250.0" layoutX="580.0" layoutY="104.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Asstes/System/logo1.png" />
         </image>
      </ImageView>
      <JFXButton fx:id="btnupload" contentDisplay="GRAPHIC_ONLY" layoutX="839.0" layoutY="298.0" onAction="#uploadimageOnAction" prefHeight="39.0" prefWidth="48.0" text="Upload ">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Asstes/System/icons8-add-image-48.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <ImageView fx:id="imgQr" fitHeight="250.0" fitWidth="250.0" layoutX="581.0" layoutY="375.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Asstes/System/logo1.png" />
         </image>
      </ImageView>
      <JFXButton fx:id="btngenerate" contentDisplay="GRAPHIC_ONLY" layoutX="843.0" layoutY="566.0" onAction="#generateQrOnAction" prefHeight="39.0" prefWidth="48.0" text="Generate">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Asstes/System/icons8-qr-50.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton fx:id="btnAdd" layoutX="347.0" layoutY="591.0" onAction="#addstudentOnAction" prefHeight="44.0" prefWidth="125.0" text="Save">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnupload1" contentDisplay="GRAPHIC_ONLY" layoutX="849.0" layoutY="308.0" onAction="#uploadimageOnAction" prefHeight="39.0" prefWidth="48.0" text="Upload ">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnNew" contentDisplay="GRAPHIC_ONLY" layoutX="455.0" layoutY="107.0" onAction="#newStudentidOnAction" prefHeight="28.0" prefWidth="54.0" text="New">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <graphic>
            <ImageView fx:id="imgNew" fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Asstes/System/icons8-add-60.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
   </children>
   <stylesheets>
      <URL value="@../../StyleSheet/System/button.css" />
      <URL value="@../../StyleSheet/System/customer.css" />
   </stylesheets>
</AnchorPane>
